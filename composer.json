{
	"name": "diecoding/yii2-pdfjs",
	"description": "Previewer PDF File with PDF.js for Yii2",
	"authors": [
		{
			"name": "Sugeng Sulistiyawan",
			"email": "sugeng.sulistiyawan@gmail.com"
		}
	],
	"type": "library",
	"keywords": [
		"yii2",
		"widget",
		"pdfjs",
		"pdfviewer",
		"jquery"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/sugeng-sulistiyawan/yii2-pdfjs/issues",
		"source": "https://github.com/sugeng-sulistiyawan/yii2-pdfjs"
	},
	"require": {
		"yiisoft/yii2": "~2.0"
	},
	"autoload": {
		"psr-4": {
			"diecoding\\pdfjs\\": "src/"
		}
	},
    "repositories": [{
        "type": "composer",
        "url": "https://asset-packagist.org"
    }],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
